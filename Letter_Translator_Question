  print(letter_translator("A")) # Should print unknown
  print(letter_translator("")) # Should print unknown



#Answer
  def letter_translator(letter):
      if letter == "a":
          letter_position = 1
      elif letter == "b":
          letter_position = 2
      elif letter == "c":
          letter_position = 3
      elif letter == "d":
          letter_position = 4
      else:
          letter_position = "unknown"
      return letter_position
  
  
  print(letter_translator("a"))
  print(letter_translator("b"))
  print(letter_translator("c"))
  print(letter_translator("d"))
  print(letter_translator("e"))
  print(letter_translator("A"))
  print(letter_translator(""))
  
    #Result: 1
    #2
    #3
    #4
    #unknown
    #unknown
    #unknown
